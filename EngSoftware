#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main () {
	 int anoatual, nasc,idade = 0;
	 char nome[10];
	 printf("Ano de nascimento:\n");
	 scanf("%d",&nasc);
	 printf("Nome:\n");
	 scanf("%s", &nome);
	 printf("Ano em que estamos:\n");
	 scanf("%d", &anoatual);
	 
	 idade =  anoatual - nasc;
	 printf("Sua idade: %d", idade);
	 
}
